        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-prettier": "^5.5.3",
        "prettier": "^3.6.2",
        "ts-node": "^10.9.2",
        "typescript": "^5.0.0",
        "typescript-eslint": "^8.37.0"
    },
    "dependencies": {
        "@pulumi/gcp": "^9.10.0",
        "@pulumi/github": "^6.11.0",
